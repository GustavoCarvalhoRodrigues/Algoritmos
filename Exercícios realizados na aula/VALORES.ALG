Algoritmo "Valores"

Var
   num_1, num_2, soma: inteiro


Inicio
   Escreva("Digite o primeiro número: ")
   leia(num_1)
   Escreva("Digite o segundo número: ")
   leia(num_2)
   soma <- num_1 + num_2
    Escreva("A soma de dois números inteiros como", num_1, " e", num_2, " será: ", soma)
Fimalgoritmo